			"dist/nodes/CxfKafka/CxfKafkaTrigger.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~5.45",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^2.7.1",
		"typescript": "~4.8.4"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@kafkajs/confluent-schema-registry": "^3.9.0",
		"kafkajs": "^2.2.4"
